{
  "family": "ots-erp-final-verify",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::928805968684:role/ots-erp-migrator-execution-role",
  "taskRoleArn": "arn:aws:iam::928805968684:role/ots-erp-migrator-task-role",
  "containerDefinitions": [
    {
      "name": "verify",
      "image": "928805968684.dkr.ecr.us-east-2.amazonaws.com/ots-erp/migrator:final-verify",
      "cpu": 256,
      "memory": 512,
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ots-erp-migrator",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "final-verify"
        }
      }
    }
  ]
}