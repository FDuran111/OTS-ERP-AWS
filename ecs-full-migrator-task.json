{
  "family": "ots-erp-full-migrator",
  "taskRoleArn": "arn:aws:iam::928805968684:role/ots-erp-prod-app-role",
  "executionRoleArn": "arn:aws:iam::928805968684:role/ots-erp-prod-app-role",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "2048",
  "memory": "4096",
  "containerDefinitions": [
    {
      "name": "full-migrator",
      "image": "928805968684.dkr.ecr.us-east-2.amazonaws.com/ots-erp/migrator:full-migration",
      "essential": true,
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "STORAGE_DRIVER",
          "value": "S3"
        },
        {
          "name": "S3_BUCKET",
          "value": "ots-erp-prod-uploads"
        },
        {
          "name": "S3_REGION",
          "value": "us-east-2"
        },
        {
          "name": "AWS_REGION",
          "value": "us-east-2"
        },
        {
          "name": "DB_DRIVER",
          "value": "RDS"
        },
        {
          "name": "RDS_ENDPOINT",
          "value": "ots-erp-prod-rds.c5cymmac2hya.us-east-2.rds.amazonaws.com"
        },
        {
          "name": "RDS_DB",
          "value": "ortmeier"
        },
        {
          "name": "RDS_USER",
          "value": "otsapp"
        }
      ],
      "secrets": [
        {
          "name": "RDS_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:928805968684:secret:ots-erp/prod/rds/password-min-xYcQKp:password::"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ots-erp-full-migrator",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}