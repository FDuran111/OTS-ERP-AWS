{
  "family": "ots-erp-migration-direct",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "executionRoleArn": "arn:aws:iam::928805968684:role/ots-erp-migrator-execution-role",
  "taskRoleArn": "arn:aws:iam::928805968684:role/ots-erp-migrator-task-role",
  "containerDefinitions": [
    {
      "name": "migrator",
      "image": "928805968684.dkr.ecr.us-east-2.amazonaws.com/ots-erp/migrator:direct",
      "cpu": 1024,
      "memory": 2048,
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ots-erp-migrator",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "migration"
        }
      }
    }
  ]
}